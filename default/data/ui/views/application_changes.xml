<dashboard>
  <label>Application Changes</label>
  <row>
    <panel>
      <table>
        <title>Deployments</title>
        <search>
          <query>eventtype="sccm_change_audit" messageid=30226 | `sccm_lookup_status_message_desc` | table _time, messageid, username, description</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="wrap">undefined</option>
        <option name="rowNumbers">undefined</option>
        <option name="drilldown">row</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>changes</title>
        <search>
          <query>eventtype="sccm_change_audit" collectionid!=null | `sccm_lookup_status_message_desc` | table _time, username, description</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="wrap">undefined</option>
        <option name="rowNumbers">undefined</option>
        <option name="drilldown">row</option>
        <option name="dataOverlayMode">none</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
</dashboard>